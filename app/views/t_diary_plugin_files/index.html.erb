<h1>TDiary plugin checker</h1>

<div id="lastupdate">
	Last Update: 2012-08-10
</div>

<table cellpadding="0" cellspacing="0">
  <tr>
    <th>Name</th>
    <th>Age</th>
    <th>Message</th>
  </tr>

<% @t_diary_plugin_files.each do |t_diary_plugin_file| %>
  <tr>
		<td><a href="<%= t_diary_plugin_file.link %>"><%= t_diary_plugin_file.path %></a></td>
    <td><%= time_ago_in_words t_diary_plugin_file.commit.date %></td>
		<td><a href="<%= t_diary_plugin_file.commit.link %>"><%= t_diary_plugin_file.commit.message %></a></td>
  </tr>
<% end %>
</table>

